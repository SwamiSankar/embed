{
  "name": "@synergetics-sdk/embed-react",
  "version": "4.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "author": "Synergetics AI",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "yarn build-embed-css && yarn webpack",
    "build-embed-css": "cd ../embed && yarn build",
    "dev": "concurrently 'yarn build --watch' 'yarn dev-vanilla'",
    "dev-vanilla": "cd ../embed && yarn dev",
    "demo-nextjs": "cd ../demo-nextjs && yarn start",
    "demo": "yarn concurrently 'yarn dev' 'yarn demo-nextjs'",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx --max-warnings=0",
    "release": "npm config set @synergetics-sdk:registry https://registry.npmjs.org/ && yarn semantic-release",
    "post-release": "yarn release:github",
    "release:github": "npm config set @synergetics-sdk:registry https://npm.pkg.github.com/ && npm publish"
  },
  "dependencies": {
    "@synergetics-sdk/embed": "5.0.0",
    "fast-deep-equal": "^3.1.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "files": [
    "build",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:/embed.git",
    "directory": "packages/embed-react"
  },
  "homepage": "https://github.com/synergetics-ai/synergetics-sdk/embed/blob/main/packages/embed-react#readme",
  "bugs": "https://github.com/synergetics-ai/synergetics-sdk/embed/issues",
  "keywords": [
    "synergetics-sdk",
    "embed",
    "form",
    "widget",
    "slider",
    "sidetab",
    "popover",
    "react"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12"
  }
}
